<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background02"
    tools:context=".bayadhago02">

    <RelativeLayout
        android:id="@+id/toolbarRl"
        android:layout_width="match_parent"
        android:layout_height="?actionBarSize"
        android:background="@drawable/shape_toolbar02">

        <ImageButton
            android:id="@+id/backBtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:background="@android:color/transparent"
            android:onClick="opencbackActivity"
            android:padding="5dp"
            android:src="@drawable/ic_back_white"
            tools:ignore="ContentDescription,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

        <TextView
            android:id="@+id/TitleTv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="البياضة"
            android:textColor="@color/white"
            android:textStyle="bold"
            android:textSize="26sp"
            tools:ignore="HardcodedText" />
    </RelativeLayout>
    <TextView
        android:id="@+id/gotounivTv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/toolbarRl"
        android:layout_centerInParent="true"
        android:layout_marginTop="20dp"
        android:text="الذهاب الى الجامعة"
        android:textColor="@color/text01"
        android:textSize="35sp"
        android:textStyle="bold"
        tools:ignore="HardcodedText" />
    <ImageButton
        android:background="@drawable/taw9it02"
        android:id="@+id/taw9it0Btn"
        android:layout_width="390dp"
        android:layout_height="50dp"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="15dp"
        android:layout_below="@+id/gotounivTv"
        android:layout_marginTop="20dp"/>

    <Button
        android:id="@+id/taw9it1Btn"
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="30dp"
        android:layout_below="@+id/taw9it0Btn"
        android:layout_marginTop="20dp"
        android:text="14"
        android:background="@drawable/shape_button01"
        android:textSize="24sp"/>
    <Button
        android:id="@+id/taw9it2Btn"
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="30dp"
        android:layout_below="@+id/taw9it1Btn"
        android:layout_marginTop="10dp"
        android:text="16"
        android:background="@drawable/shape_button01"
        android:textSize="24sp"/>
    <Button
        android:id="@+id/taw9it3Btn"
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="30dp"
        android:layout_below="@+id/taw9it2Btn"
        android:layout_marginTop="10dp"
        android:text="17"
        android:background="@drawable/shape_button01"
        android:textSize="24sp"/>
    <Button
        android:id="@+id/taw9it4Btn"
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="30dp"
        android:layout_below="@+id/taw9it3Btn"
        android:layout_marginTop="10dp"
        android:text="18"
        android:background="@drawable/shape_button01"
        android:textSize="24sp"/>
    <Button
        android:id="@+id/taw9it5Btn"
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="30dp"
        android:layout_below="@+id/taw9it4Btn"
        android:layout_marginTop="10dp"
        android:text="77"
        android:background="@drawable/shape_button01"
        android:textSize="24sp"/>
    <Button
        android:id="@+id/taw9it6Btn"
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="30dp"
        android:layout_below="@+id/taw9it5Btn"
        android:layout_marginTop="10dp"
        android:text="92"
        android:background="@drawable/shape_button01"
        android:textSize="24sp"/>



</RelativeLayout>